{
    "Define component": {
        "prefix":"component",
        "body": [
            "(component $1 ($2)",
            "   $3",
            ")"
        ],
        "description": "Define a custom component"
    },
    "Basic page": {
        "prefix": "page",
        "body": [
            "(head",
            "   (meta charset=\"UTF-8\")",
            "   (meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\")",
            "   ",
            "   (title \"$1\")",
            ")",
            "(body",
            "   $2",
            ")"
        ],
        "description": "The very basics to start work on a MEML page"
    }
}